#include <stm32f405xx.h>
uint8_t Sensorstatus;

int main(void) {
  RCC->AHB1ENR |= (1 << 1);
  GPIOB->MODER &= ~(3 << 14);

  RCC->AHB1ENR |= (1 << 1);
  GPIOB->MODER &= ~(3 << 12);
  GPIOB->MODER |= (1 << 12);

  while (1) {
    if ((GPIOB->IDR) & (1 << 7)) {
      GPIOB->ODR |= (1 << 6);
    }
    else {
      GPIOB->ODR &= ~(1 << 6);
    }
  }
  return 0;
}
